# Emergency Azure App Service Configuration
# This forces Azure to use startup.py instead of main.py

# Use startup.py to bypass all main.py caching issues
PORT=8000
WEBSITES_PORT=8000

# Force Azure to use our startup script
WEBSITES_CONTAINER_START_TIME_LIMIT=900

# Application command override
PYTHON_DEFAULT_EXECUTABLE=python3
PYTHON_STARTUP_FILE=startup.py

# Cache bypass settings
SCM_CACHE_CLEAR=true
SCM_REPOSITORY_CACHE_CLEAR=true
WEBSITE_LOCAL_CACHE_OPTION=Never
WEBSITE_LOCAL_CACHE_SIZEINMB=0
WEBSITE_DYNAMIC_CACHE=0
WEBSITE_RUN_FROM_PACKAGE=0